@model IEnumerable<Week5Lab.Models.Order>
@{
	ViewData["Title"] = "Test";
}

<h2>Test</h2>
<table class="table">
	<thead>
		<tr>
			<th>ID</th>
			<th>Name</th>
			<th>Price</th>
			<th>Amount</th>
			


		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{

			<tr>
				<td>
					@item.Id
				</td>
				<td>
					@item.Name
				</td>
				<td>
					@item.Price
				</td>
				<td>
					@item.Amount
				</td>
				



			</tr>
		}
	</tbody>
</table>
<h2>Total Amount </h2>
<h3>@ViewData["TotalAmount"]</h3>

